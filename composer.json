{
  "name": "yzh52521/think-lock",
  "type": "library",
  "license": "MIT",
  "description": "thinkphp lock plugin",
  "require": {
    "php": "^7.2|^8.0",
    "symfony/lock": "^5.0|^6.0",
    "illuminate/container":"^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "yzh52521\\ThinkLock\\": "src"
    }
  },
  "require-dev": {
    "topthink/framework": "^6|^8"
  },
  "extra": {
    "think": {
      "config": {
        "lock": "src/config.php"
      }
    }
  }
}