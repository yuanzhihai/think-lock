{
  "name": "yzh52521/think-lock",
  "type": "library",
  "license": "MIT",
  "description": "thinkphp lock plugin",
  "require": {
    "php": "^7.2|^8.0",
    "symfony/lock": "^5.0|^6.0",
    "topthink/framework": ">=6.1.2"
  },
  "autoload": {
    "psr-4": {
      "yzh52521\\ThinkLock\\": "src"
    }
  },
  "extra": {
    "think": {
      "config": {
        "lock": "src/config.php"
      }
    }
  }
}